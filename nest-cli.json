{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/guardian-theater/src",
  "monorepo": true,
  "root": "apps/guardian-theater",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/guardian-theater/tsconfig.app.json"
  },
  "projects": {
    "guardian-theater": {
      "type": "application",
      "root": "apps/guardian-theater",
      "entryFile": "main",
      "sourceRoot": "apps/guardian-theater/src",
      "compilerOptions": {
        "tsConfigPath": "apps/guardian-theater/tsconfig.app.json"
      }
    },
    "activity-harvester": {
      "type": "application",
      "root": "apps/activity-harvester",
      "entryFile": "main",
      "sourceRoot": "apps/activity-harvester/src",
      "compilerOptions": {
        "tsConfigPath": "apps/activity-harvester/tsconfig.app.json"
      }
    },
    "shared-services": {
      "type": "library",
      "root": "libs/shared-services",
      "entryFile": "index",
      "sourceRoot": "libs/shared-services/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-services/tsconfig.lib.json"
      }
    },
    "mixer-name-matcher": {
      "type": "application",
      "root": "apps/mixer-name-matcher",
      "entryFile": "main",
      "sourceRoot": "apps/mixer-name-matcher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mixer-name-matcher/tsconfig.app.json"
      }
    },
    "twitch-name-matcher": {
      "type": "application",
      "root": "apps/twitch-name-matcher",
      "entryFile": "main",
      "sourceRoot": "apps/twitch-name-matcher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/twitch-name-matcher/tsconfig.app.json"
      }
    }
  }
}